MAIN.C
#include "Euler.h"
#include <stdio.h>
#include "Function.h"


int main()
{
	printf("%llu", Problem_2(4000000));
}




EULER.C
#include "Euler.h"
#include <stdbool.h>
#include <stdio.h>
#include "Function.h"
unsigned long long Problem_2(const int value_)
{
	unsigned long long sum = 0;
	unsigned int n1 = 1;
	unsigned int n2 = 2;
	while (n2 < value_)
	{
		if (n2 % 2 == 0) {
			sum += n2;
		}
		unsigned int temp = n1;
		n1 = n2;
		n2 = n2 + temp;

	}
	return sum;
}

