MAIN.C
#include "Euler.h"
#include <stdio.h>
#include "Function.h"


int main()
{
	printf("%llu", Problem_4());
}


FUNCTION.C
#include "Function.h"
#include <stdbool.h>
#include "math.h"
bool IsPalendrom(const unsigned int number_)
{
	unsigned int original = number_;
	unsigned int revers = 0;

	while (original > 0)
	{
		unsigned char d = original % 10;
		revers = revers * 10 + d;
		original /= 10;
	}
	return number_ == revers;
}


EULER.C
#include "Euler.h"
#include <stdbool.h>
#include <stdio.h>
#include "Function.h"
unsigned long long Problem_4()
{
	unsigned int result = 0;
	for (unsigned int n_1 = 100; n_1 < 1000; n_1++)
	{
		for (unsigned int n_2 = 100; n_2 < 1000; n_2++)
		{
			unsigned int mult = n_1 * n_2;
			if (IsPalendrom(mult))
			{
				if (result < mult)
					result = mult;
			}
		}
	}
	return result;
}
